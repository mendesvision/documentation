{
  "openapi": "3.1.0",
  "info": {
    "title": "Eyo Wallet API",
    "description": "API para integração com a Eyo Wallet - Plataforma de pagamentos PIX para desenvolvedores e empresas",
    "version": "1.0.0",
    "contact": {
      "name": "Eyo Wallet",
      "url": "https://eyowallet.com"
    }
  },
  "servers": [
    {
      "url": "https://api.eyowallet.com",
      "description": "Servidor de Produção"
    }
  ],
  "security": [
    {
      "apiKeyAuth": []
    }
  ],
  "tags": [
    {
      "name": "Payments",
      "description": "Endpoints para criar e gerenciar pagamentos PIX"
    },
    {
      "name": "Withdraws",
      "description": "Endpoints para criar e gerenciar saques via PIX"
    },
    {
      "name": "Transfers",
      "description": "Endpoints para transferências internas entre contas Eyo Wallet"
    },
    {
      "name": "User",
      "description": "Endpoints para informações do usuário e configurações"
    }
  ],
  "paths": {
    "/api/v1/payment/create": {
      "post": {
        "operationId": "createPayment",
        "summary": "Criar Pagamento",
        "description": "Cria uma nova transação PIX e retorna um QR Code para pagamento",
        "tags": [
          "Payments"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreatePaymentRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Pagamento criado com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreatePaymentResponse"
                }
              }
            }
          },
          "400": {
            "description": "Erro de validação",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "401": {
            "description": "API Key inválida ou ausente",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "429": {
            "description": "Rate limit excedido",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/payment/get/{id}": {
      "get": {
        "operationId": "getPayment",
        "summary": "Buscar Pagamento",
        "description": "Consulta os detalhes de uma transação PIX específica. Este endpoint sincroniza automaticamente o status com o provedor de pagamento.",
        "tags": [
          "Payments"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "ID único da transação de pagamento",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Pagamento encontrado",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetPaymentResponse"
                }
              }
            }
          },
          "404": {
            "description": "Pagamento não encontrado",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/payment/list": {
      "get": {
        "operationId": "listPayments",
        "summary": "Listar Pagamentos",
        "description": "Lista todas as transações PIX do usuário com filtros e paginação",
        "tags": [
          "Payments"
        ],
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "description": "Filtrar por status",
            "schema": {
              "type": "string",
              "enum": [
                "PENDING",
                "ACTIVE",
                "COMPLETED",
                "FAILED"
              ]
            }
          },
          {
            "name": "limit",
            "in": "query",
            "description": "Número máximo de resultados (máximo 100)",
            "schema": {
              "type": "integer",
              "default": 50,
              "maximum": 100
            }
          },
          {
            "name": "offset",
            "in": "query",
            "description": "Número de resultados a pular (para paginação)",
            "schema": {
              "type": "integer",
              "default": 0
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Lista de pagamentos",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ListPaymentsResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/withdraw/create": {
      "post": {
        "operationId": "createWithdraw",
        "summary": "Criar Saque",
        "description": "Cria um novo saque para transferir dinheiro via PIX. O saldo é debitado imediatamente.",
        "tags": [
          "Withdraws"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateWithdrawRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Saque criado com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateWithdrawResponse"
                }
              }
            }
          },
          "400": {
            "description": "Erro de validação ou saldo insuficiente",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/withdraw/crypto": {
      "post": {
        "operationId": "createCryptoWithdraw",
        "summary": "Criar Saque Crypto",
        "description": "Cria um novo saque em criptomoeda (USDT BEP20).",
        "tags": [
          "Withdraws"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateCryptoWithdrawRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Saque crypto adicionado à fila",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateCryptoWithdrawResponse"
                }
              }
            }
          },
          "400": {
            "description": "Erro de validação, saldo insuficiente ou wallet inválida",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/withdraw/get/{id}": {
      "get": {
        "operationId": "getWithdraw",
        "summary": "Buscar Saque",
        "description": "Consulta os detalhes de um saque específico",
        "tags": [
          "Withdraws"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "ID único do saque",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Saque encontrado",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetWithdrawResponse"
                }
              }
            }
          },
          "404": {
            "description": "Saque não encontrado",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/withdraw/list": {
      "get": {
        "operationId": "listWithdraws",
        "summary": "Listar Saques",
        "description": "Lista todos os saques do usuário com filtros e paginação",
        "tags": [
          "Withdraws"
        ],
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "description": "Filtrar por status",
            "schema": {
              "type": "string",
              "enum": [
                "PENDING",
                "COMPLETED",
                "FAILED"
              ]
            }
          },
          {
            "name": "limit",
            "in": "query",
            "description": "Número máximo de resultados (máximo 100)",
            "schema": {
              "type": "integer",
              "default": 50,
              "maximum": 100
            }
          },
          {
            "name": "offset",
            "in": "query",
            "description": "Número de resultados a pular (para paginação)",
            "schema": {
              "type": "integer",
              "default": 0
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Lista de saques",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ListWithdrawsResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/transfer/internal": {
      "post": {
        "operationId": "internalTransfer",
        "summary": "Transferência Interna",
        "description": "Transfere saldo para outra conta Eyo Wallet usando o email. Transferências internas são gratuitas e instantâneas.",
        "tags": [
          "Transfers"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InternalTransferRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Transferência realizada com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InternalTransferResponse"
                }
              }
            }
          },
          "400": {
            "description": "Saldo insuficiente ou transferência para si mesmo",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "404": {
            "description": "Usuário não encontrado",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/user/balance": {
      "get": {
        "operationId": "getBalance",
        "summary": "Consultar Saldo",
        "description": "Obtém informações detalhadas sobre saldo, estatísticas e limites da conta",
        "tags": [
          "User"
        ],
        "responses": {
          "200": {
            "description": "Informações do saldo",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BalanceResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "webhooks": {
    "payment.approved": {
      "post": {
        "summary": "Pagamento Aprovado",
        "description": "Enviado quando um pagamento PIX é confirmado e aprovado",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PaymentApprovedWebhook"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Webhook recebido com sucesso"
          }
        }
      }
    },
    "payment.expired": {
      "post": {
        "summary": "Pagamento Expirado",
        "description": "Enviado quando um pagamento expira sem ser pago",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PaymentExpiredWebhook"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Webhook recebido com sucesso"
          }
        }
      }
    },
    "payment.refunded": {
      "post": {
        "summary": "Pagamento Reembolsado",
        "description": "Enviado quando um pagamento é reembolsado",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PaymentRefundedWebhook"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Webhook recebido com sucesso"
          }
        }
      }
    },
    "withdrawal.completed": {
      "post": {
        "summary": "Saque Processado",
        "description": "Enviado quando um saque é processado com sucesso",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/WithdrawalCompletedWebhook"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Webhook recebido com sucesso"
          }
        }
      }
    },
    "withdrawal.failed": {
      "post": {
        "summary": "Saque Falhou",
        "description": "Enviado quando um saque falha",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/WithdrawalFailedWebhook"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Webhook recebido com sucesso"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "apiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-Key",
        "description": "API Key obtida no painel da Eyo Wallet"
      }
    },
    "schemas": {
      "CreatePaymentRequest": {
        "type": "object",
        "required": [
          "value"
        ],
        "properties": {
          "value": {
            "type": "number",
            "description": "Valor da transação em reais. Mínimo: R$ 1,00. Máximo: R$ 1.000,00 por transação.",
            "minimum": 1,
            "maximum": 1000
          },
          "description": {
            "type": "string",
            "description": "Descrição opcional da transação"
          },
          "coverFee": {
            "type": "boolean",
            "default": false,
            "description": "Se true, o valor informado é o valor líquido que você deseja receber. A taxa será adicionada ao valor total."
          },
          "splitUser": {
            "type": "string",
            "description": "Email de outro usuário Eyo Wallet para dividir o pagamento"
          },
          "splitTax": {
            "type": "number",
            "default": 50,
            "minimum": 1,
            "maximum": 100,
            "description": "Porcentagem do valor a ser transferida para o usuário de split"
          }
        }
      },
      "CreatePaymentResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "ID único do pagamento"
              },
              "transactionId": {
                "type": "string",
                "description": "ID da transação na gateway"
              },
              "value": {
                "type": "integer",
                "description": "Valor total em centavos"
              },
              "valueInReais": {
                "type": "number",
                "description": "Valor total em reais"
              },
              "netValue": {
                "type": "integer",
                "description": "Valor líquido após dedução da taxa, em centavos"
              },
              "fee": {
                "type": "integer",
                "description": "Taxa cobrada em centavos"
              },
              "status": {
                "type": "string",
                "enum": [
                  "PENDING",
                  "ACTIVE",
                  "COMPLETED",
                  "FAILED"
                ]
              },
              "qrcodeUrl": {
                "type": "string",
                "description": "QR Code em formato base64 (data URI)"
              },
              "copyPaste": {
                "type": "string",
                "description": "Código PIX para copiar e colar"
              },
              "createdAt": {
                "type": "string",
                "format": "date-time"
              }
            }
          }
        }
      },
      "GetPaymentResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "statusUpdated": {
            "type": "boolean",
            "description": "Indica se o status foi atualizado durante a consulta"
          },
          "data": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "transactionId": {
                "type": "string"
              },
              "value": {
                "type": "integer"
              },
              "netValue": {
                "type": "integer"
              },
              "status": {
                "type": "string",
                "enum": [
                  "PENDING",
                  "ACTIVE",
                  "COMPLETED",
                  "FAILED"
                ]
              },
              "qrCode": {
                "type": "string",
                "nullable": true
              },
              "description": {
                "type": "string"
              },
              "createdAt": {
                "type": "string",
                "format": "date-time"
              },
              "updatedAt": {
                "type": "string",
                "format": "date-time"
              }
            }
          }
        }
      },
      "ListPaymentsResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "data": {
            "type": "object",
            "properties": {
              "payments": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/PaymentItem"
                }
              },
              "pagination": {
                "$ref": "#/components/schemas/Pagination"
              },
              "statistics": {
                "$ref": "#/components/schemas/PaymentsStatistics"
              }
            }
          }
        }
      },
      "PaymentItem": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "correlationID": {
            "type": "string"
          },
          "value": {
            "type": "integer"
          },
          "netValue": {
            "type": "integer"
          },
          "fee": {
            "type": "integer"
          },
          "status": {
            "type": "string",
            "enum": [
              "PENDING",
              "ACTIVE",
              "COMPLETED",
              "FAILED"
            ]
          },
          "description": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "CreateWithdrawRequest": {
        "type": "object",
        "required": [
          "amount",
          "pixKey",
          "pixKeyType"
        ],
        "properties": {
          "amount": {
            "type": "number",
            "description": "Valor do saque em reais. Mínimo: R$ 5,00",
            "minimum": 5
          },
          "pixKey": {
            "type": "string",
            "description": "Chave PIX do destinatário"
          },
          "pixKeyType": {
            "type": "string",
            "enum": [
              "CPF",
              "CNPJ",
              "EMAIL",
              "PHONE",
              "RANDOM"
            ],
            "description": "Tipo da chave PIX"
          },
          "description": {
            "type": "string",
            "description": "Descrição opcional do saque"
          },
          "coverFee": {
            "type": "boolean",
            "default": false,
            "description": "Se true, o valor informado é o valor líquido que será enviado"
          },
          "verificationCode": {
            "type": "string",
            "description": "Código de verificação se a segurança de transferências estiver ativada"
          },
          "splitUser": {
            "type": "string",
            "description": "Email de outro usuário Eyo Wallet para dividir o saque"
          },
          "splitTax": {
            "type": "number",
            "default": 50,
            "minimum": 1,
            "maximum": 100,
            "description": "Porcentagem do valor a ser transferida para o usuário de split"
          }
        }
      },
      "CreateWithdrawResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "value": {
                "type": "integer",
                "description": "Valor total em centavos"
              },
              "fee": {
                "type": "integer",
                "description": "Taxa em centavos"
              },
              "sent": {
                "type": "integer",
                "description": "Valor líquido enviado em centavos"
              },
              "status": {
                "type": "string",
                "enum": [
                  "PENDING",
                  "COMPLETED",
                  "FAILED"
                ]
              },
              "pixKey": {
                "type": "string"
              },
              "createdAt": {
                "type": "string",
                "format": "date-time"
              }
            }
          }
        }
      },
      "CreateCryptoWithdrawRequest": {
        "type": "object",
        "required": [
          "amount",
          "wallet"
        ],
        "properties": {
          "amount": {
            "type": "number",
            "description": "Valor do saque em reais. Mínimo: R$ 20,00. Máximo: R$ 3.000,00",
            "minimum": 20,
            "maximum": 3000
          },
          "wallet": {
            "type": "string",
            "description": "Endereço da wallet BEP20 (formato: 0x seguido de 40 caracteres hexadecimais)",
            "pattern": "^0x[a-fA-F0-9]{40}$"
          },
          "description": {
            "type": "string",
            "description": "Descrição opcional do saque"
          }
        }
      },
      "CreateCryptoWithdrawResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "ID único do saque"
              },
              "type": {
                "type": "string",
                "enum": [
                  "CRYPTO_BEP20"
                ],
                "description": "Tipo do saque"
              },
              "currency": {
                "type": "string",
                "enum": [
                  "USDT"
                ],
                "description": "Moeda"
              },
              "network": {
                "type": "string",
                "enum": [
                  "BEP20"
                ],
                "description": "Rede blockchain"
              },
              "value": {
                "type": "integer",
                "description": "Valor em centavos"
              },
              "valueInReais": {
                "type": "number",
                "description": "Valor em reais"
              },
              "fee": {
                "type": "integer",
                "description": "Taxa em centavos (6% + R$ 2,00)"
              },
              "feeInReais": {
                "type": "number",
                "description": "Taxa em reais"
              },
              "wallet": {
                "type": "string",
                "description": "Endereço da wallet BEP20"
              },
              "status": {
                "type": "string",
                "enum": [
                  "QUEUED",
                  "PROCESSING",
                  "COMPLETED",
                  "FAILED"
                ],
                "description": "Status do saque"
              },
              "jobId": {
                "type": "string",
                "description": "ID do job na fila de processamento"
              },
              "createdAt": {
                "type": "string",
                "format": "date-time"
              }
            }
          }
        }
